<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="eqlee.ctm.finance.settlement.dao.NumberDetailedMapper">

    <insert id="insertNumberDetailed" parameterType="eqlee.ctm.finance.settlement.entity.vo.ContectUserNumberVo">
        insert into NumberDetailed (Id,ContectUserName,ContectUserTel,AllNumber,AllPrice,BabyNumber,OldNumber,ChildNumber,AdultNumber)
        <foreach collection="list" item="item" index="index" separator="union all">
            select #{item.Id},#{item.ContectUserName},#{item.ContectUserTel},#{item.AllNumber},#{item.AllPrice},
            #{item.BabyNumber},#{item.OldNumber},#{item.ChildNumber},#{item.AdultNumber}
        </foreach>
    </insert>

</mapper>